{"ast":null,"code":"import { format } from 'date-fns';\nvar DATE_FORMAT = 'MM/dd/yyyy';\nvar TIME_FORMAT = 'HH:mm';\nexport var formatDate = function formatDate(date) {\n  return format(new Date(date), DATE_FORMAT);\n};\nexport var formatTime = function formatTime(date) {\n  return format(new Date(date), TIME_FORMAT);\n};\nexport var formatDateRange = function formatDateRange(range) {\n  var from = range.from,\n      to = range.to;\n  return \"\".concat(formatDate(from), \" \").concat(formatTime(from), \" - \").concat(formatTime(to));\n};","map":null,"metadata":{},"sourceType":"module"}